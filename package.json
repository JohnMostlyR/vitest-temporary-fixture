{
  "name": "vitest-temporary-fixture",
  "version": "1.2.0",
  "description": "Create temporary directories and fixtures on the fly for Vitest tests.",
  "keywords": [
    "vitest",
    "testing",
    "fixtures",
    "temporary",
    "temp",
    "test-helpers",
    "test-utils",
    "directory",
    "filesystem",
    "mock-filesystem",
    "test-fixtures",
    "cleanup"
  ],
  "homepage": "https://github.com/JohnMostlyR/vitest-temporary-fixture",
  "bugs": {
    "url": "https://github.com/JohnMostlyR/vitest-temporary-fixture/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JohnMostlyR/vitest-temporary-fixture.git"
  },
  "license": "MIT",
  "author": "Johan Meester",
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "clean": "git clean -xdf .cache dist node_modules",
    "coverage": "vitest run --coverage",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "eslint --fix .",
    "prepack": "unbuild",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  },
  "dependencies": {
    "mkdirp": "^3.0.1"
  },
  "devDependencies": {
    "@eslint/compat": "1.4.0",
    "@ianvs/prettier-plugin-sort-imports": "4.7.0",
    "@types/node": "24.9.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "9.38.0",
    "eslint-plugin-sonarjs": "3.0.5",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0",
    "unbuild": "3.6.1",
    "unique-string": "3.0.0",
    "vitest": "3.2.4"
  },
  "peerDependencies": {
    "vitest": ">=3.0.0"
  }
}
